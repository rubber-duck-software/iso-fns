{
  "opts": {
    "template": "node_modules/better-docs"
  },
  "tags": {
    "allowUnknownTags": ["optional"]
  },
  "recurseDepth": 10,
  "plugins": ["node_modules/better-docs/typescript"],
  "source": {
    "include": [".", "README.md"],
    "exclude": [".github", ".vscode", "coverage", "dist", "node_modules"],
    "includePattern": "\\.(ts|tsx)$"
  }
}
