{
  "name": "iso-fns",
  "description": "A string-based date-time library for Javascript.",
  "version": "0.0.26",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "iso-fns.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/rubber-duck-software/iso-fns.git"
  },
  "scripts": {
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "build": "tsc",
    "test": "yarn lint && c8 -r none beartest 'dist/**/*.test.js'",
    "cc": "c8 report -r lcovonly",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "docs": "jsdoc -c jsdoc.json -r -d ./docs",
    "verify-build": "yarn format && yarn build && yarn test && yarn cc && yarn docs"
  },
  "dependencies": {
    "dateformat": "^4.5.1"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "all-contributors-cli": "^6.20.0",
    "beartest-js": "^3.1.1",
    "better-docs": "^2.3.2",
    "c8": "^7.7.3",
    "coveralls": "^3.1.1",
    "expect": "^27.0.6",
    "jsdoc": "^3.6.7",
    "prettier": "^2.3.2",
    "typescript": "^4.3.2"
  },
  "exports": {
    ".": "./dist/index.js",
    "./DayOfWeek": "./dist/DayOfWeek/index.js",
    "./DayOfWeek/*": "./dist/DayOfWeek/*.js",
    "./Instant": "./dist/Instant/index.js",
    "./Instant/*": "./dist/Instant/*.js",
    "./LocalDate": "./dist/LocalDate/index.js",
    "./LocalDate/*": "./dist/LocalDate/*.js",
    "./LocalDateTime": "./dist/LocalDateTime/index.js",
    "./LocalDateTime/*": "./dist/LocalDateTime/*.js",
    "./LocalTime": "./dist/LocalTime/index.js",
    "./LocalTime/*": "./dist/LocalTime/*.js",
    "./Month": "./dist/Month/index.js",
    "./Month/*": "./dist/Month/*.js",
    "./MonthDay": "./dist/MonthDay/index.js",
    "./MonthDay/*": "./dist/MonthDay/*.js",
    "./Year": "./dist/Year/index.js",
    "./Year/*": "./dist/Year/*.js",
    "./YearMonth": "./dist/YearMonth/index.js",
    "./YearMonth/*": "./dist/YearMonth/*.js",
    "./package.json": "./package.json"
  }
}
